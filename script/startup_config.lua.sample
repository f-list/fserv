-- Redis configuration
enableredis=true
redishost="127.0.0.1"
redisport=1234
redispass="alongandcomplicatedrandomizedpassword"

-- General daemon configuration
bindaddr="0.0.0.0"
version="0.8.6-Lua"
port=9722
maxusers=2000
loginslots=30
saveinterval=300
websocketorigin="http://www.f-list.net"
websockethost="www.f-list.net"

-- Trusted load balancer hosts
load_balancers={"127.0.0.1", "10.20.1.1"}

-- Logging
log_start=false
log_socket_path="/tmp/fserv_log.sock"

--- Enable Real-Time Bridge
enablertb=true
--- Address that RTB messages can come from.
rtbaddress="127.0.0.1"
--- RTB MAC secret.
rtbsecret="anotherlongandcomplicatedrandomizedpassword"


-- Logging
log_start=false
log_socket_path="/home/kira/fserv/bin/log.sock"

--Login (evHTTPClient)
logintimeout=30
---Number of concurrent connections to the login server.
login_connection_pool=15
login_url="absolute url to login verification script"


-- Chat Report Handling
handle_report_url="absolute url to handling script"
handle_report_secret="handle report secret"

-- Lua
--- Should lua time out event handlers.
luaenabletimeout=true
--- The number of seconds before handlers initially time out in the event loop
luatimeout=0.25
--- The number of seconds before a handler times out if one has already timed out that event loop
luarepeattimeout=0.05


-- Chat limits
max_per_ip=3
msg_max=4096
priv_max=50000
lfrp_max=50000
cds_max=50000

-- Chat throttles
msg_flood=0.5
sta_flood=5
lfrp_flood=600
kinks_flood=10
profile_flood=10
find_flood=10
staffcall_flood=120
ignore_flood=30

-- Blacklist phrases
blacklist_phrases={"newts are not cute"}
